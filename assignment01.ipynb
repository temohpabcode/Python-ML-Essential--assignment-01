    "# 2-D Array\n",
    "import numpy as np\n",
    "\n",
    "# create two-dimensional array\n",
    "def create_array(m, n):\n",
    "  return np.arange(m*n).reshape(m,n) if m >= 0 and n >= 0 else \"Not negative values\"\n",
    "\n",
    "# driver code\n",
    "m = 2\n",
    "n = -1\n",
    "\n",
    "create_array(m, n)\n"
   ]
